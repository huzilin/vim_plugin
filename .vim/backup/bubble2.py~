def bubble(num_list):
    for i in range(len(num_list)):
        for j in range(len(num_list)-i-1):
            if num_list[j] > num_list[j+1]:
                num_list[j], num_list[j+1] = num_list[j+1], num_list[j]


num_list = [3,5,22,1,2,6,3,7]

bubble(num_list)

for each in num_list:
    print each','
