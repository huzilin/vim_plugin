import re
ip = ['10.12.16.224', '10.256.8.56', '192.168.257.1', '172.56.25.2', '183.18.46.53', '86.17.46.12', '213.49.56.38']


a = r'((2[0-4]\d|25[0-5]|[01]?\d\d?)\.){3}(2[0-4]\d|25[0-5]|[01]?\d\d?)'
correct_ip = []
for each in ip:
    if re.match(a, each):
        correct_ip.append(each)

m1 = r'10\..*|192\..*|172\..*'
intranet_ips = []
extranet_ips = []
for each in correct_ip:
    if re.match(m1, each):
        intranet_ips.append(each)
    else:
        extranet_ips.append(each)

print intranet_ips
print extranet_ips
