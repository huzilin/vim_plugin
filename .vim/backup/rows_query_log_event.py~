import sys

sys.path.append("..")
from include.unpack_byte import *


class Rows_Query_Log_Event:
    """
    29
    sql_len_len = 1
    """
    def __init__(self, post_buf):
        self.parse = unpack_byte.Parse_Byte_Stream(post_buf)
        self.len = self.parse.get_uint8()
        self.sql = self.parse.get_char(len(post_buf) - 1)

    def event_output(self):
        print "sql: %s" % self.sql

