CREATE TABLE `SGPMS_LN`.`A_RCVED_PL_FLOW` (`RCVED_PL_ID` DECIMAL(16,0) NOT NULL ,`RCVED_AMT_ID` DECIMAL(16,0) DEFAULT NULL ,`RCVED_YM` VARCHAR(6) DEFAULT NULL ,`ITEM_CODE` VARCHAR(8) DEFAULT NULL ,`PL_AMT` DECIMAL(18,2) DEFAULT NULL ,`ACCT_NO` VARCHAR(16) DEFAULT NULL ,`ORG_NO` VARCHAR(16) DEFAULT NULL ,`RCVBL_YM` VARCHAR(6) DEFAULT NULL ,`PART_CHG_YM` VARCHAR(6) DEFAULT NULL );
DROP TABLE IF EXISTS `SGPMS_LN`.`ARC_A_RCVED_PL_FLOW`;
CREATE TABLE `SGPMS_LN`.`ARC_A_RCVED_PL_FLOW` (`RCVED_PL_ID` DECIMAL(16,0) DEFAULT NULL ,`RCVED_AMT_ID` DECIMAL(16,0) DEFAULT NULL ,`RCVED_YM` VARCHAR(6) DEFAULT NULL ,`ITEM_CODE` VARCHAR(8) DEFAULT NULL ,`PL_AMT` DECIMAL(18,2) DEFAULT NULL ,`ACCT_NO` VARCHAR(16) DEFAULT NULL ,`ORG_NO` VARCHAR(16) DEFAULT NULL );
