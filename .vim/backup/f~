package main

import (
	"flag"
	"fmt"
)

var (
	installpath *string
	mysql       string
)

func init_options() {
	installpath := flag.String("installpath", "/", "Install path")
	var mysql string
	flag.StringVar(&mysql, "mysql", "/mysql", "mysql")
	fmt.Printf("%v\n", *installpath)
	fmt.Printf("%v\n", mysql)
}

func main() {
	init_options()
	flag.Parse()
	args := flag.Args()
	fmt.Printf("%v\n", args)
	fmt.Printf("%s", installpath)
}
