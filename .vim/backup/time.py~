#!/usr/bin/python
#coding:utf-8

import struct

log = open('/opt/mysql-5.5/data/mybinlog.000005', 'rb')
log.seek(8903+19+6+2+1+1+1)

a, b, c = struct.unpack('BBB',log.read(3))
ret = a + (b << 8) + (c << 16)
print ret
if ret:
    value = '%02d:%02d:%02d' % ((ret % 1000000) / 10000,
                                (ret % 10000) / 100,
                                (ret % 100))
else:
    value = "00:00:00"
print value
