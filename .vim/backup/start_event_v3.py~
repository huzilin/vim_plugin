
class Start_Event_V3:

    """
    1
    binlog_ver_len = 2
    created_timestamp = 4
    """
    server_ver_len = 50

    def __init__(self, post_buf):
        self.parse = unpack_byte.Parse_Byte_Stream(post_buf)

        self.binlog_ver = self.parse.get_uint16()
        self.server_ver = self.parse.get_char(Start_Event_V3.server_ver_len)
        self.created_timestamp = self.parse.get_uint32()
