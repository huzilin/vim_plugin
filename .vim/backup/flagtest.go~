package main

import (
	"flag"
	"fmt"
)

var (
	install_path *string
	mysql        string
)

func init_options() {
	install_path := flag.String("install_path", "/", "Install path")
	var mysql string
	flag.StringVar(&mysql, "mysql", "/mysql", "mysql")
	fmt.Printf("%v\n", *install_path)
	fmt.Printf("%v\n", mysql)
}

func main() {
	init_options()
	/*
		flag.Parse()
		args := flag.Args()
		fmt.Printf("%v\n", args)
		fmt.Printf("%v\n", *install_path)
	*/
}
