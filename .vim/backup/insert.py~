def insert_sort(lists):
    count = len(lists)
    for i in range(1, count):
        key = lists[i]
        j = i - 1
        while j >= 0:
            if lists[j] > key:
                lists[j + 1] = lists[j]
                lists[j] = key
                print lists
            j -= 1
    return lists

lists = [5,2,1,10,22,7]
insert_sort(lists)
print lists
